<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>Drylungs</title>
	<subtitle>Records & Mailorder</subtitle>
	<link href="http://drylungs.at/feed" rel="self"/>
	<link href="http://drylungs.at" />
	::foreach records::
	<entry>
		<title>::id::</title>
		<link href="http://drylungs.at/records/::id::" />
		::if description::<summary>::description::</summary>::end::
		<content type="xhtml">
			<div xmlns="http://www.w3.org/1999/xhtml">
				<h1>::id::</h1>
				<h2>::name::</h2>
				::if artists::::foreach artists::<li class="artist">::__current__::</li>::end::::end::
				<img class="cover" src="http://drylungs.at/image/record/::id::.jpg">
			</div>
		</content>
		<author>
			<name>Christoph</name>
			<email>christoph@drylungs.at</email>
		</author>
	</entry>
	::end::
</feed>
